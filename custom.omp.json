{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "foreground": "cyan",
          "type": "path",
          "style": "plain",
          "template": "{{ .Path }}",
          "options": {
            "style": "full",
            "mapped_locations_enabled": false
          }
        },
        {
          "template": " git:({{ .HEAD }})",
          "foreground": "#6c6c6c",
          "type": "git",
          "style": "plain"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "template": "âžœ ",
          "foreground": "green",
          "type": "text",
          "style": "plain"
        }
      ],
      "newline": true
    }
  ],
  "version": 3
}
